<!--Equipement -->
<div class="d-flex flex-wrap">

    <!--repose pied-->
    {% if auto.footrest == 96 %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/design/repose-pied-alu.jpg"
                    alt="repose pied"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> Repose pied </strong> <br> 96 €</p>
        </div>

    {% endif %}

    <!-- pack heritage -->
    {% if auto.heritagePack == 180 %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/design/pack-heritage.jpg"
                    alt="repose pied"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> Pack Heritage</strong> 180 €</p>
        </div>

    {% endif %}

    <!-- aide stationnement arriere -->
    {% if auto.helpParkingBack == 420  %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/conduite/aide-stationnement-ar.jpg"
                    alt="repose pied"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> aide stationnement arrière</strong>
                <br>
                420 €
            </p>
        </div>

    {% endif %}

    <!-- aide stationnement avant -->
    {% if auto.helpParkingBefore == 750  %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/conduite/aide-stationnement-av-ar.jpg"
                    alt="repose pied"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> aide stationnement avant </strong> 750 €</p>
        </div>

    {% endif %}

    <!-- caméra de recul -->
    {% if auto.reverseCamera == 1200  %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/conduite/camera-recul.jpg"
                    alt="camera de recul"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> camera de recul  </strong> <br>1200 €</p>
        </div>

    {% endif %}


    <!-- échappement sport -->
    {% if auto.sportExhaust == 1500  %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/conduite/echappement-sport.jpg"
                    alt="camera de recul"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> echapement sport  </strong> <br> 1 500 €</p>
        </div>

    {% endif %}


    <!-- pack rangement -->
    {% if auto.storagePack == 504 %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/confort/pack-rangement.jpg"
                    alt="camera de recul"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> Pack rangement  </strong> <br> 504 €</p>
        </div>

    {% endif %}


    <!-- régulateur de vitesse -->
    <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
        <img
                src="/picture/configurateur/equipements/categories/confort/regul-limit-vitesse.jpg"
                alt="camera de recul"
                class="imageRelatif img-fluid mt-3"
        />
        <p class="mt-3"><strong> régulateur de vitesse </strong> <br> 0 €</p>
    </div>



    <!-- rétro exterieur -->
    {% if auto.outsideRetro == 504  %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/confort/retro-ext-chaffant.jpg"
                    alt="retro exterieur"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> retro exterieur </strong> <br> 504 €</p>
        </div>

    {% endif %}

    <!-- rétro interieur -->

    <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
        <img
                src="/picture/configurateur/equipements/categories/confort/retro-int-electrochrome.jpg"
                alt="retro interieur"
                class="imageRelatif img-fluid mt-3"
        />
        <p class="mt-3"><strong> retro interieur </strong> <br> 0 €</p>
    </div>



    <!-- audio focal -->
    {% if auto.focalSound == 600 %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/media et navigation/audio-focal.jpg"
                    alt="audio focal"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> audio focal </strong> <br> 600 €</p>
        </div>

    {% endif %}

    <!-- audio premium -->
    {% if auto.premiumAudio == 1200 %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/media et navigation/audio-premium.jpg"
                    alt="audio Premium ALpin"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> audio premium </strong> <br> 1 200 €</p>
        </div>

    {% endif %}

    <!--standard -->
    {% if auto.premiumAudio != 1200  and auto.focalSound != 600  %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/media et navigation/audio-standard.jpg"
                    alt="audio focal"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> audio standard </strong> <br> 0 €</p>
        </div>

    {% endif %}


    <!--Alpine Metrics -->
    {% if auto.alpineMetrics == 204  %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/media et navigation/alpine-metrics.jpg"
                    alt="audio focal"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> alpine metrics </strong> <br> 204 €</p>
        </div>
    {% endif %}

    <!--Aide freinage -->
    {% if auto.highPerformanceBrake != 1000 and auto.brakeAssist == 0  %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/securite/aide-freinage-durgence.jpg"
                    alt="aide freinage"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> aide freinage </strong> <br> 0 €</p>
        </div>
    {% endif %}

    <!--Aide freinage d'urgence -->
    {% if auto.highPerformanceBrake == 1000  %}
        <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
            <img
                    src="/picture/configurateur/equipements/categories/securite/freinage-haute-perf.jpg"
                    alt="aide freinage"
                    class="imageRelatif img-fluid mt-3"
            />
            <p class="mt-3"><strong> freinage haute perfomance</strong> <br> 1 000 €</p>
        </div>
    {% endif %}


    <!--Airbag -->
    <div class="elementAuto col-12 col-sm-12 col-md-5 col-lg-3 col-xl-3">
        <img
                src="/picture/configurateur/equipements/categories/securite/airbag.jpg"
                alt="aide freinage"
                class="imageRelatif img-fluid mt-3"
        />
        <p class="mt-3"><strong> airBag </strong> <br> 0 €</p>
    </div>
</div>
<!--Repose Pied -->






